{
  "name": "jquery.validate",
  "title": "Form validation plugin",
  "description": "This plugin contains a set of validation rules, assigned to each field by special attributes and classes (eg `<input type="email" required maxlength="100" />`)",
  "version": "1.3.0",
  "homepage": "https://github.com/duzun/jquery.validate",
  "author": {
    "name": "Dumitru Uzun",
    "email": "contact@duzun.me",
    "url": "https://DUzun.Me"
  },
  "types": "validate.d.ts",
  "module": "validate.js",
  "main": "dist/validate.js",
  "unpkg": "dist/validate.min.js",
  "reveal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/duzun/jquery.validate.git"
  },
  "bugs": "https://github.com/duzun/jquery.validate/issues",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/duzun/jquery.validate/blob/master/LICENSE-MIT"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/jasmine": "^3.6.3",
    "@types/jquery": "^3.5.5",
    "jasmine-core": "^3.6.0",
    "jquery": "^3.5.1",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.4.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "verup": "^1.7"
  },
  "scripts": {
    "tsc": "npx tsc --build",
    "rollup": "npx rollup -c rollup.config.js",
    "build": "npm run tsc && npm run rollup",
    "test": "karma start",
    "verup": "verup"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "jquery",
    "validation",
    "browser",
    "form",
    "promise",
    "asynchronously",
    "async"
  ],
  "extra": {
    "verup": {
      "files": [
        "validate.esm.ts"
      ]
    }
  },
  "jshintConfig": {
    "esversion": 10,
    "laxbreak": true,
    "laxcomma": true,
    "undef": true,
    "unused": true,
    "sub": true,
    "expr": true,
    "bitwise": false,
    "eqeqeq": false,
    "boss": true,
    "eqnull": true,
    "scripturl": true,
    "-W041": false
  }
}
